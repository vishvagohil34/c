#include<stdio.h>
int main(){
	int i,n,search,found=0;
	printf("enter value of n : ");
	scanf("%d",&n);
	int a[n];
	printf("enter element of array : ");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("array is : ");
	for(i=0;i<n;i++)
	{
	    printf("%d",a[i]);
	}
	printf("enter the elemnt you want to search : ");
	scanf("%d"&search);
	for(i=0;i<n;i++)
	{
	    if(a[i]==search)
	    found = 1;
	    break;
	}
	if(found)
	printf("\nelement %d is found in an array",search);
	else 
	printf("\nelement %d is not found in an array",search");
	return=0
}